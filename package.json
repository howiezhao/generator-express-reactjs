{
  "name": "generator-express-reactjs",
  "version": "0.0.0",
  "description": "Yeoman generator for Express and ReactJS",
  "homepage": "https://howiezhao.github.io/generator-express-reactjs",
  "author": {
    "name": "howiezhao",
    "email": "howiezhaohr@hotmail.com",
    "url": "https://howiezhao.github.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "express",
    "react"
  ],
  "devDependencies": {
    "yeoman-test": "^6.3.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.7",
    "eslint": "^9.15.0",
    "prettier": "^1.19.1",
    "husky": "^9.1.6",
    "lint-staged": "^9.4.3",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-config-xo": "^0.27.2",
    "jest": "^29.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^5.7.0",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "howiezhao/generator-express-reactjs",
  "license": "Apache-2.0"
}
